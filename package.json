{
  "name": "lambda-local-runner",
  "version": "1.0.1",
  "description": "Run one or more AWS Lambda handlers locally via Express",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lambda-local-runner": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typedoc": "typedoc --entryPoints src/index.ts --out docs --excludePrivate --excludeExternals",
    "prepare": "husky"
  },
  "keywords": [
    "aws",
    "lambda",
    "local",
    "serverless",
    "express"
  ],
  "author": "Adrian Sudbury",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asudbury/lambda-local-runner.git"
  },
  "bugs": {
    "url": "https://github.com/asudbury/lambda-local-runner/issues"
  },
  "homepage": "https://asudbury.github.io/lambda-local-runner",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "body-parser": "^2.2.1",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@types/aws-lambda": "^8.10.159",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "axios": "^1.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  }
}
